{
  "buildCommand": "pnpm build",
  "outputDirectory": "apps/web/dist",
  "framework": "vite",
  "rewrites": [
    {
      "source": "/api/todos/(.*)",
      "destination": "/api/todos/$1"
    },
    {
      "source": "/api/todos",
      "destination": "/api/todos"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ],
  "functions": {
    "api/todos": {
      "memory": 128,
      "maxDuration": 10
    }
  }
}
